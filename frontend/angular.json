{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "packageManager": "yarn",
        "analytics": false
    },
    "newProjectRoot": "projects",
    "projects": {
        "gen-ai-reflection": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "gr",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "outputPath": {
                            "base": "dist",
                            "browser": ""
                        },
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": ["zone.js", "@angular/localize/init"],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.png",
                            "src/favicon.svg",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss",
                            {
                                "input": "src/dark.scss",
                                "bundleName": "dark",
                                "inject": false
                            },
                            {
                                "input": "src/light.scss",
                                "bundleName": "light",
                                "inject": false
                            }
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "node_modules/bootstrap",
                                "node_modules/uu-bootstrap"
                            ],
                            "sass": {
                                "silenceDeprecations": ["import"]
                            }
                        },
                        "scripts": [],
                        "server": "src/main.server.ts",
                        "prerender": true,
                        "ssr": {
                            "entry": "server.ts"
                        },
                        "baseHref": "/"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "ssr": false,
                            "prerender": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "2mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all",
                            "sourceMap": true
                        },
                        "development": {
                            "ssr": false,
                            "prerender": false,
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        },
                        "nl": {
                            "ssr": false,
                            "prerender": false,
                            "localize": ["nl"]
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "gen-ai-reflection:build:production"
                        },
                        "development": {
                            "buildTarget": "gen-ai-reflection:build:development"
                        },
                        "nl": {
                            "buildTarget": "gen-ai-reflection:build:nl"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "proxyConfig": "../proxy.conf.json"
                    }
                },
                "extract-i18n": {
                    "builder": "ng-extract-i18n-merge:ng-extract-i18n-merge",
                    "options": {
                      "buildTarget": "gen-ai-reflection:build",
                      "format": "xlf",
                      "outputPath": "locale",
                      "includeContext": true,
                      "targetFiles": [
                        "messages.nl.xlf"
                      ],
                      "prettyNestedTags": true,
                      "sort": "stableAppendNew"
                    }
                },
                "test": {
                    "builder": "@angular/build:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing",
                            "@angular/localize/init"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.png",
                            "src/favicon.svg",
                            "src/assets"
                        ],
                        "styles": ["src/styles.scss"],
                        "scripts": [],
                        "karmaConfig": "karma.conf.js",
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "node_modules/bootstrap",
                                "node_modules/uu-bootstrap"
                            ],
                            "sass": {
                                "silenceDeprecations": ["import"]
                            }
                        }
                    }
                }
            },
            "i18n": {
                "sourceLocale": {
                    "code": "en"
                },
                "locales": {
                    "nl": {
                        "translation": "locale/messages.nl.xlf"
                    }
                }
            }
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "type": "component"
        },
        "@schematics/angular:directive": {
            "type": "directive"
        },
        "@schematics/angular:service": {
            "type": "service"
        },
        "@schematics/angular:guard": {
            "typeSeparator": "."
        },
        "@schematics/angular:interceptor": {
            "typeSeparator": "."
        },
        "@schematics/angular:module": {
            "typeSeparator": "."
        },
        "@schematics/angular:pipe": {
            "typeSeparator": "."
        },
        "@schematics/angular:resolver": {
            "typeSeparator": "."
        }
    }
}
